{
  "name": "react-starter",
  "version": "1.0.0",
  "description": "React + React Router + Webpack 2 + Mocha + Chai",
  "main": "dist/index.html",
  "engines": {
    "npm": "3.10.3",
    "node": "6.3.0"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --config src/config/webpack/webpack.config.js",
    "lint": "eslint --config src/config/eslint/eslint.config.json src/js/**",
    "lint:test": "eslint --config src/config/eslint/eslint.config.json test/js/**",
    "lint:config": "eslint --config src/config/eslint/eslint.config.json src/config/**",
    "lint:server": "eslint --config src/config/eslint/eslint.config.json server/**",
    "setup": "node src/config/prompts/setup.js",
    "start": "cross-env NODE_ENV=development webpack-dev-server --config src/config/webpack/webpack.config.js",
    "start:build": "node server/server.js",
    "stats": "cross-env NODE_ENV=production webpack --config src/config/webpack/webpack.config.js --profile --json > webpack.stats.json",
    "test": "ava"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/neosavvyinc/react-starter.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/neosavvyinc/react-starter/issues"
  },
  "homepage": "https://github.com/neosavvyinc/react-starter#readme",
  "ava": {
    "babel": "inherit",
    "files": [
      "test/js"
    ],
    "require": [
      "./test/config/testSetup.js"
    ]
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ],
    "plugins": [
      "transform-decorators-legacy",
      "transform-class-properties"
    ]
  },
  "devDependencies": {
    "autoprefixer": "6.4.0",
    "ava": "0.16.0",
    "babel-eslint": "6.1.2",
    "babel-loader": "6.2.4",
    "babel-plugin-transform-class-properties": "6.11.5",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-react": "6.11.1",
    "clean-webpack-plugin": "0.1.10",
    "cross-env": "3.1.4",
    "css-loader": "0.23.1",
    "cssnano": "3.7.3",
    "enzyme": "2.4.1",
    "eslint": "3.2.0",
    "eslint-loader": "1.5.0",
    "eslint-plugin-react": "5.2.2",
    "express": "4.14.0",
    "extract-text-webpack-plugin": "2.0.0-beta.3",
    "fetch-mock": "5.0.5",
    "file-loader": "0.9.0",
    "html-webpack-plugin": "2.22.0",
    "image-webpack-loader": "2.0.0",
    "isomorphic-fetch": "2.2.1",
    "lodash": "4.14.1",
    "mobx-react-devtools": "4.2.4",
    "node-sass": "3.8.0",
    "postcss-loader": "0.9.1",
    "prompt": "1.0.0",
    "react-addons-test-utils": "15.3.0",
    "sass-loader": "4.0.0",
    "sinon": "1.17.5",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "2.1.0-beta.20",
    "webpack-dev-server": "2.1.0-beta.0",
    "webpack-merge": "0.14.1",
    "webpack-validator": "2.2.7"
  },
  "dependencies": {
    "mobx": "2.4.1",
    "mobx-provide": "1.0.3",
    "mobx-react": "3.5.3",
    "react": "15.3.0",
    "react-dom": "15.3.0",
    "react-router": "2.6.0"
  }
}
